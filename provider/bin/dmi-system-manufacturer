#!/usr/bin/env python3

import subprocess
import sys

# List of manufacturers.
manufacturers = [
    'System76',
    'System76, Inc.'
]

result = subprocess.run(['dmidecode', '-s', 'system-manufacturer'], stdout=subprocess.PIPE)
result.check_returncode()
manufacturer = result.stdout.decode('utf-8').strip()

if manufacturer in manufacturers:
    print(manufacturer)
else:
    print('Unknown: %s' % (manufacturer))
    sys.exit(1)
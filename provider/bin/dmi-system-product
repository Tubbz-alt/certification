#!/usr/bin/env python3

import subprocess
import sys

# List of products.
products = [
    'Bonobo WS',
    'Galago Pro',
    'Gazelle',
    'Kudu',
    'Lemur',
    'Oryx Pro',
    'Serval WS'
]

fp = open('/sys/class/dmi/id/product_name')
product = fp.read().strip()
fp.close()

if product in products:
    print(product)
else:
    print('Unknown: %s' % (product))
    sys.exit(1)
